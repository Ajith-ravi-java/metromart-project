package com.Ecommerce.site.entity;

import java.util.UUID;


import jakarta.persistence.Column;
import jakarta.persistence.Entity;
import jakarta.persistence.GeneratedValue;
import jakarta.persistence.Id;

@Entity
public class userDetails {
	
	@Id
    @GeneratedValue
    @Column(columnDefinition = "BINARY(16)")
	private UUID user_id;
	
	private String username;
	
	private String password;
	
	private String city;
	
	@Column(columnDefinition = "VARCHAR(20)")
	private marritalStatus marritalstatus;
	
	private userStatus status;

	public UUID getUser_id() {
		return user_id;
	}

	public void setUser_id(UUID user_id) {
		this.user_id = user_id;
	}

	public String getUsername() {
		return username;
	}

	public void setUsername(String username) {
		this.username = username;
	}

	public String getPassword() {
		return password;
	}

	public void setPassword(String password) {
		this.password = password;
	}

	public String getCity() {
		return city;
	}

	public void setCity(String city) {
		this.city = city;
	}

	

	public marritalStatus getMarritalstatus() {
		return marritalstatus;
	}

	public void setMarritalstatus(marritalStatus marritalstatus) {
		this.marritalstatus = marritalstatus;
	}

	public userStatus getStatus() {
		return status;
	}

	public void setStatus(userStatus status) {
		this.status = status;
	}

	public userDetails(UUID user_id, String username, String password, String city, marritalStatus marritalstatus,userStatus status) {
		super();
		this.user_id = user_id;
		this.username = username;
		this.password = password;
		this.city = city;
		this.marritalstatus=marritalstatus;
		this.status = status;
	}

	public userDetails() {
		super();
	}

	
}
